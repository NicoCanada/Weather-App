{
  "name": "@weatherio/city-data",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/city-list.json",
    "./admin1": "./src/admin1.json",
    "./admin2": "./src/admin2.json"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint \"**/*.mjs\"",
    "typecheck": "tsc --noEmit",
    "update:city-data": "cd src && node download.mjs && node convert.mjs && node renamejsonproperties.mjs"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@weatherio/eslint-config/base"
    ]
  },
  "prettier": "@weatherio/prettier-config",
  "dependencies": {
    "jsonfile": "^6.1.0",
    "yauzl": "^2.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonfile": "^6.1.4",
    "@types/yauzl": "^2.10.3",
    "@weatherio/eslint-config": "workspace:^0.2.0",
    "@weatherio/prettier-config": "workspace:^0.1.0",
    "@weatherio/tsconfig": "workspace:^0.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
