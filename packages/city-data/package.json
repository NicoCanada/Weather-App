{
  "name": "@weatherio/city-data",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./admin1": "./src/admin1.json",
    "./admin2": "./src/admin2.json"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "update:city-data": "cd src && tsx download.ts && tsx convert.ts",
    "push:city-data-prod": "pnpm with-env tsx src/push-city-data.ts",
    "push:city-data-dev": "",
    "with-env": "dotenv -e ../../.env --"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@weatherio/eslint-config/base"
    ]
  },
  "prettier": "@weatherio/prettier-config",
  "dependencies": {
    "@weatherio/db": "workspace:^",
    "dotenv-cli": "^7.4.1",
    "jsonfile": "^6.1.0",
    "yauzl": "^3.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonfile": "^6.1.4",
    "@types/yauzl": "^2.10.3",
    "@weatherio/eslint-config": "workspace:^0.2.0",
    "@weatherio/prettier-config": "workspace:^0.1.0",
    "@weatherio/tsconfig": "workspace:^0.1.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  }
}
